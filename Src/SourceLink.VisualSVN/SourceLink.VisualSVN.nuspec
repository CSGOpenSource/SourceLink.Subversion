<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>$Id$</id>
    <version>$Version$</version>
    <authors>$Authors$</authors>
    <description>Generates source link for VisualSVN repositories.</description>
    <dependencies>
      <dependency id="Microsoft.SourceLink.Common" version="1.0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="$ProjectDirectory$\..\..\Tool\x86\*.*" target="tools/x86" />
    <file src="$ProjectDirectory$\..\..\Tool\x64\*.*" target="tools/x64" />
    <file src="$ProjectDirectory$\bin\release\net472\SourceLink.SVN.dll" target="tools/net472" />
    <file src="$ProjectDirectory$\bin\release\net472\SourceLink.SVN.pdb" target="tools/net472" />
    <file src="$ProjectDirectory$\bin\release\netcoreapp2.1\SourceLink.SVN.dll" target="tools/netcoreapp2.1" />
    <file src="$ProjectDirectory$\bin\release\netcoreapp2.1\SourceLink.SVN.pdb" target="tools/netcoreapp2.1" />
    
    <file src="$ProjectDirectory$\build\*.*" target="build" />
    <file src="$ProjectDirectory$\buildMultiTargeting\*.*" target="buildMultiTargeting" />
  </files>
</package>